<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>ParlatorServlet</servlet-name>
        <servlet-class>com.intergrammar.parlator.ParlatorServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>ParlatorServlet</servlet-name>
        <url-pattern>/parla</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>VoicesServlet</servlet-name>
        <servlet-class>com.intergrammar.parlator.VoicesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>VoicesServlet</servlet-name>
        <url-pattern>/voices</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ErrorMessageServlet</servlet-name>
        <servlet-class>com.intergrammar.parlator.ErrorMessageServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>ErrorMessageServlet</servlet-name>
        <url-pattern>/errorMessage</url-pattern>
    </servlet-mapping>
</web-app>
